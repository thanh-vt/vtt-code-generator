<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<fx:root spacing="10.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <Label alignment="CENTER" prefHeight="25.0" styleClass="b" text="Path" />
  <TextField fx:id="outPath" minWidth="250.0" />
  <Button mnemonicParsing="false" onAction="#browse" text="Browse">
    <styleClass>
      <String fx:value="btn" />
      <String fx:value="btn-default" />
      <String fx:value="btn-sm" />
    </styleClass>
  </Button>
</fx:root>
